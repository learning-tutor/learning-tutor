{
  "name": "react-app1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-exposed": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start-watch": "tsc && vite --watch",
    "prettier-format": "prettier --write src/",
    "docker:image": "docker build -t react-app1 .",
    "docker:local": "docker run --rm -p 8080:80 -v ./app:/App -w /App react-app1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test-ui": "vitest --ui"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@linaria/core": "^6.1.0",
    "@linaria/react": "^6.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "@wyw-in-js/babel-preset": "^0.2.3",
    "D": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@storybook/addon-essentials": "7.6.5",
    "@storybook/addon-interactions": "7.6.5",
    "@storybook/addon-links": "7.6.5",
    "@storybook/addon-onboarding": "1.0.10",
    "@storybook/blocks": "7.6.5",
    "@storybook/react": "7.6.5",
    "@storybook/react-vite": "7.6.5",
    "@storybook/test": "7.6.5",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.59",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "@wyw-in-js/vite": "^0.2.3",
    "axe-core": "^4.8.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest-axe": "^8.0.0",
    "jsdom": "^24.0.0",
    "prettier": "3.1.0",
    "semantic-release": "^22.0.12",
    "storybook": "7.6.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@4.0.2",
  "resolutions": {
    "@vitejs/plugin-react": "4.2.1"
  }
}
